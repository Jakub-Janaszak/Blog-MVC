<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.lab3_v8.kontroler.LoginController">
   <children>
      <Label fx:id="blog" layoutX="257.0" layoutY="51.0" text="LOGIN">
         <font>
            <Font name="Bodoni MT Poster Compressed" size="60.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldUsername" layoutX="226.0" layoutY="187.0">
         <tooltip>
            <Tooltip text="enter username" />
         </tooltip></TextField>
      <TextField fx:id="textFieldPassword" accessibleRole="PASSWORD_FIELD" layoutX="226.0" layoutY="235.0">
         <tooltip>
            <Tooltip text="enter password" />
         </tooltip></TextField>
      <Button fx:id="buttonLogin" layoutX="350.0" layoutY="294.0" mnemonicParsing="false" onAction="#switchToSideMenu" text="login">
         <tooltip>
            <Tooltip text="login" />
         </tooltip></Button>
      <Label layoutX="226.0" layoutY="169.0" text="username:" />
      <Label layoutX="226.0" layoutY="217.0" text="password:" />
      <Label fx:id="labelError" layoutX="286.0" layoutY="271.0" textAlignment="CENTER" textFill="RED" />
      <Button fx:id="backToMenu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMainMenu" text="back to menu">
         <tooltip>
            <Tooltip text="back to menu" />
         </tooltip></Button>
   </children>
</AnchorPane>
